
<div style="margin:50px;">
	<h1><%= @user.name %></h1>
	<h3>Edit User</h3>
	<%= form_tag update_user_path do %>
			<%=text_field_tag :name, @user.name %><br />
			<%= number_field_tag :age, @user.age %><br />
			<%= hidden_field_tag :id, @user.id %><br />
			<%= submit_tag "Edit Profile" %>


	<% end %>

	<p><%= link_to "All Users", users_path %></p>



</div>